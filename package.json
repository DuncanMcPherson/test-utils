{
  "name": "@ez-budgets/test-utils",
  "version": "1.0.3",
  "description": "Packaging meant to help with the process of writing Jest tests",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "'./dist/index.d.ts",
  "files": [
    "/dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest --config=./jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanMcPherson/test-utils.git"
  },
  "keywords": [
    "jest",
    "testing",
    "mocking"
  ],
  "author": "Duncan McPherson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanMcPherson/test-utils/issues"
  },
  "jest-junit": {
    "outputDirectory": "./test-results",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": ">"
  },
  "homepage": "https://github.com/DuncanMcPherson/test-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "jest-junit": "^16.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@ngx-translate/core": "^15.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1"
  }
}
